{
    "html": {
        "brace_style": "collapse",
        "indent_char": " ",
        "indent_scripts": "keep",
        "indent_size": 4,
        "unformatted": ["a", "sub", "sup", "b", "i", "u"],
        "wrap_line_length": 0,
        "end_with_newline": true
    },
    "css": {
        "indent_char": " ",
        "indent_size": 4
    },
    "js": {
        "indent_size": 4,
        "indent_char": " ",
        "indent_level": 0,
        "indent_with_tabs": false,
        "preserve_newlines": true,
        "max_preserve_newlines": 4,
        "jslint_happy": true
    }
}
